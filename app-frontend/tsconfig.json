{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
      "@/*": [
        "node_modules/mhr-payload/src/*"
      ],
      "@payload-config": [
        "node_modules/mhr-payload/src/payload.config.ts"
      ],
    }
  }
}
