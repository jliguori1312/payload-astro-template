    "./vue/*": "./src/blocks/*/vue/index.vue",
    "./config/*": "./src/blocks/*/config.ts",
    "./component/*": "./src/components/*",
    "./field/*": "./src/fields/*.ts"
  },
  "imports": {
    "#util/*": "./src/utilities/*",
    "#blocks/*": "./src/blocks/*",
    "#fields/*": "./src/fields/*",
    "#components/*": "./src/components/*",
    "#config": "./src/config.ts"
  },
  "devDependencies": {
    "@payloadcms/plugin-form-builder": "catalog:payload",
    "@payloadcms/ui": "^3.37.0",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@tailwindcss/postcss": "^4.1.3",
    "@types/node": "22.13.8",
    "@types/react": "catalog:react",
    "@types/react-dom": "catalog:react",
    "app-payload": "workspace:*",
    "astro": "^5.6.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.477.0",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.1.0",
    "react-hook-form": "7.54.2",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.3",
    "tsx": "^4.19.3",
    "typescript": "5.8.2",
    "vue": "^3.5.13"
  },
  "peerDependencies": {
    "@payloadcms/plugin-form-builder": "catalog:payload",
    "@payloadcms/richtext-lexical": "catalog:payload",
    "@payloadcms/ui": "^3.37.0",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "1.2.2",
    "astro": "^5.6.1",
    "class-variance-authority": "0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.477.0",
    "payload": "catalog:payload",
    "prism-react-renderer": "^2.4.1",
    "react-hook-form": "7.54.2",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "astro": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "tailwindcss": {
      "optional": false
    }
  },
  "dependencies": {
    "payload-admin-bar": "^1.0.7"
  }
