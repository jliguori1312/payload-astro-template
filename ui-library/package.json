{
  "name": "ui-library",
  "version": "0.0.1",
  "description": "Block library for Payload CMS and many frontend frameworks.",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "generate": "tsx generate-exports.ts"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.7.1",
  "exports": {
    ".": "./src/config.ts",
    "./react/*": "./src/blocks/*/react/index.tsx",
